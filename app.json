{
  "expo": {
    "name": "Befree",
    "slug": "befree",
    "description": "Break free from your bad habits with Befree. Say goodbye to old patterns and embrace personal transformation.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.emigrek.befree",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "userInterfaceStyle": "automatic",
      "package": "com.emigrek.befree",
      "permissions": ["HIGH_SAMPLING_RATE_SENSORS"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "6a0af6a7-06f7-4017-84e0-6974291edf74"
      }
    },
    "scheme": "befree",
    "plugins": [
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to allow you to set your personal image as your addiction image."
        }
      ]
    ]
  }
}
